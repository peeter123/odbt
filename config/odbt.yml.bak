### Octopart DBlib Tools Configuration Settings
odbt:
  ### Toggle application level debug (does not toggle framework debugging)
  # debug: false

  ### Where external (third-party) plugins are loaded from
  # plugin_dir: /var/lib/odbt/plugins/

  ### Where all plugin configurations are loaded from
  # plugin_config_dir: /etc/odbt/plugins.d/

  ### Where external templates are loaded from
  # template_dir: /var/lib/odbt/templates/

  ### The log handler label
  # log_handler: colorlog

  ### The output handler label
  # output_handler: jinja2

  ### Octopart API key to access the Octopart V3 API
  octopart_api_key: '69ec4b8e'

  ### Digikey API key to access the Digikey API
  digikey_client_id: '293d958a-cc1d-44e6-9a84-742c8ca27b0e'
  digikey_client_secret: 'sH5wR2pJ0yP7nP8tO7pR8vO3wW1mG3mD6xT2aJ8aV7jX6gW6bR'
  digikey_cache_directory: 'D:\Work\GIT\altium-dblib-octopart-populator\cache'

  ### Altium DBLib Access Database path
  db_path: 'C:\Work\Altium_Data\ICT Library\ICT.mdb'

  ### Standard database fields
  db_designer: 'P. Oostewechel'

  ### Supplier choices for standard Altium database fields
  db_supplier_1: 'Farnell'
  db_supplier_1_base_url: 'https://nl.farnell.com/'
  db_supplier_2: 'Digi-Key'
  db_supplier_2_base_url: 'https://www.digikey.com/'

log.colorlog:
  ### Where the log file lives (no log file by default)
  # file: null

  ### The level for which to log.  One of: info, warning, error, fatal, debug
  level: debug

  ### Whether or not to log to console
  to_console: true

  ### Whether or not to rotate the log file when it reaches `max_bytes`
  # rotate: false

  ### Max size in bytes that a log file can grow until it is rotated.
  # max_bytes: 512000

  ### The maximun number of log files to maintain when rotating
  # max_files: 4
